{
	"$schema": "vscode://schemas/color-theme",
	"name": "DeepDark",
	"type": "dark",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#C9D1D9",
		"editorLineNumber.foreground": "#4A5568",
		"editorLineNumber.activeForeground": "#60A5FA",
		"editor.lineHighlightBackground": "#0A0E14",
		"editor.selectionBackground": "#0F1B2E",
		"editor.inactiveSelectionBackground": "#0F1B2E80",
		"editor.selectionHighlightBackground": "#0F1B2E60",
		"editorCursor.foreground": "#3B82F6",
		"editorWhitespace.foreground": "#4A556840",
		"editorIndentGuide.background1": "#4A556820",
		"editorIndentGuide.activeBackground1": "#3B82F660",
		
		"sideBar.background": "#000000",
		"sideBar.foreground": "#C9D1D9",
		"sideBarTitle.foreground": "#60A5FA",
		"sideBarSectionHeader.background": "#0A0E14",
		"sideBarSectionHeader.foreground": "#60A5FA",
		"sideBarSectionHeader.border": "#1A3A3A",
		
		"activityBar.background": "#000000",
		"activityBar.foreground": "#60A5FA",
		"activityBar.inactiveForeground": "#4A5568",
		"activityBar.border": "#404040",
		"activityBarBadge.background": "#3B82F6",
		"activityBarBadge.foreground": "#0A0E14",
		
		"panel.background": "#000000",
		"panel.border": "#404040",
		"panelTitle.activeBorder": "#3B82F6",
		"panelTitle.activeForeground": "#60A5FA",
		"panelTitle.inactiveForeground": "#4A5568",
		
		"statusBar.background": "#0A0E14",
		"statusBar.foreground": "#60A5FA",
		"statusBar.border": "#404040",
		"statusBar.debuggingBackground": "#3B82F6",
		"statusBar.debuggingForeground": "#0A0E14",
		"statusBar.noFolderBackground": "#0A0E14",
		
		"tab.activeBackground": "#000000",
		"tab.activeForeground": "#60A5FA",
		"tab.inactiveBackground": "#000000",
		"tab.inactiveForeground": "#6B7280",
		"tab.border": "#000000",
		"tab.activeBorder": "#3B82F6",
		"tab.unfocusedActiveBorder": "#4A5568",
		"tab.lastPinnedBorder": "#1A3A3A",
		
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroupHeader.noTabsBackground": "#000000",
		
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#60A5FA",
		"titleBar.inactiveBackground": "#000000",
		"titleBar.inactiveForeground": "#4A5568",
		"titleBar.border": "#404040",
		
		"input.background": "#0A0E14",
		"input.foreground": "#C9D1D9",
		"input.border": "#1A3A3A",
		"input.placeholderForeground": "#6B7280",
		"inputOption.activeBackground": "#3B82F6",
		"inputOption.activeForeground": "#0A0E14",
		"inputOption.activeBorder": "#3B82F6",
		
		"dropdown.background": "#0A0E14",
		"dropdown.foreground": "#C9D1D9",
		"dropdown.border": "#1A3A3A",
		
		"button.background": "#3B82F6",
		"button.foreground": "#0A0E14",
		"button.hoverBackground": "#60A5FA",
		"button.secondaryBackground": "#0A0E14",
		"button.secondaryForeground": "#C9D1D9",
		"button.secondaryHoverBackground": "#1A3A3A",
		
		"list.activeSelectionBackground": "#1A3A3A",
		"list.activeSelectionForeground": "#60A5FA",
		"list.inactiveSelectionBackground": "#1A3A3A80",
		"list.inactiveSelectionForeground": "#C9D1D9",
		"list.hoverBackground": "#0D111720",
		"list.hoverForeground": "#C9D1D9",
		"list.focusBackground": "#1A3A3A",
		"list.focusForeground": "#60A5FA",
		"list.dropBackground": "#1A3A3A80",
		"list.highlightForeground": "#3B82F6",
		
		"menu.background": "#0A0E14",
		"menu.foreground": "#C9D1D9",
		"menu.selectionBackground": "#1A3A3A",
		"menu.selectionForeground": "#60A5FA",
		"menu.separatorBackground": "#1A3A3A",
		"menu.border": "#1A3A3A",
		
		"scrollbarSlider.background": "#4A556820",
		"scrollbarSlider.hoverBackground": "#4A556840",
		"scrollbarSlider.activeBackground": "#3B82F660",
		
		"badge.background": "#3B82F6",
		"badge.foreground": "#0A0E14",
		
		"progressBar.background": "#3B82F6",
		
		"editorWidget.background": "#0A0E14",
		"editorWidget.foreground": "#C9D1D9",
		"editorWidget.border": "#1A3A3A",
		"editorSuggestWidget.background": "#0A0E14",
		"editorSuggestWidget.foreground": "#C9D1D9",
		"editorSuggestWidget.border": "#1A3A3A",
		"editorSuggestWidget.highlightForeground": "#3B82F6",
		"editorSuggestWidget.selectedBackground": "#1A3A3A",
		
		"editorHoverWidget.background": "#0A0E14",
		"editorHoverWidget.foreground": "#C9D1D9",
		"editorHoverWidget.border": "#1A3A3A",
		
		"peekView.border": "#3B82F6",
		"peekViewEditor.background": "#000000",
		"peekViewResult.background": "#0A0E14",
		"peekViewTitle.background": "#0A0E14",
		
		"notifications.background": "#0A0E14",
		"notifications.foreground": "#C9D1D9",
		"notifications.border": "#1A3A3A",
		"notificationLink.foreground": "#3B82F6",
		
		"breadcrumb.background": "#000000",
		"breadcrumb.foreground": "#6B7280",
		"breadcrumb.focusForeground": "#C9D1D9",
		"breadcrumb.activeSelectionForeground": "#3B82F6",
		"breadcrumbPicker.background": "#0A0E14",
		
		"terminal.background": "#000000",
		"terminal.foreground": "#C9D1D9",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiRed": "#F87171",
		"terminal.ansiGreen": "#22C55E",
		"terminal.ansiYellow": "#FCD34D",
		"terminal.ansiBlue": "#38BDF8",
		"terminal.ansiMagenta": "#A78BFA",
		"terminal.ansiCyan": "#06B6D4",
		"terminal.ansiWhite": "#C9D1D9",
		"terminal.ansiBrightBlack": "#4A5568",
		"terminal.ansiBrightRed": "#FCA5A5",
		"terminal.ansiBrightGreen": "#4ADE80",
		"terminal.ansiBrightYellow": "#FDE68A",
		"terminal.ansiBrightBlue": "#7DD3FC",
		"terminal.ansiBrightMagenta": "#C4B5FD",
		"terminal.ansiBrightCyan": "#22D3EE",
		"terminal.ansiBrightWhite": "#F3F4F6",
		
		"gitDecoration.modifiedResourceForeground": "#FCD34D",
		"gitDecoration.deletedResourceForeground": "#F87171",
		"gitDecoration.untrackedResourceForeground": "#22C55E",
		"gitDecoration.ignoredResourceForeground": "#6B7280",
		"gitDecoration.conflictingResourceForeground": "#A78BFA",
		
		"editorError.foreground": "#F87171",
		"editorWarning.foreground": "#FCD34D",
		"editorInfo.foreground": "#38BDF8",
		"editorHint.foreground": "#3B82F6",
		
		"diffEditor.insertedTextBackground": "#22C55E20",
		"diffEditor.removedTextBackground": "#F8717120",
		
		"minimap.background": "#000000",
		"minimapSlider.background": "#1A3A3A40",
		"minimapSlider.hoverBackground": "#1A3A3A60",
		"minimapSlider.activeBackground": "#1A3A3A80",
		
		"charts.red": "#F87171",
		"charts.blue": "#38BDF8",
		"charts.yellow": "#FCD34D",
		"charts.orange": "#FB923C",
		"charts.green": "#22C55E",
		"charts.purple": "#A78BFA",
		
		"textLink.foreground": "#3B82F6",
		"textLink.activeForeground": "#60A5FA",
		
		"checkbox.background": "#0A0E14",
		"checkbox.foreground": "#C9D1D9",
		"checkbox.border": "#1A3A3A",
		
		"widget.border": "#1A3A3A"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6B7280",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#A5F3FC"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#A5F3FC"
			}
		},
		{
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#CCFBF1"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#FEF08A"
			}
		},
		{
			"scope": [
				"constant.language.boolean",
				"constant.language.true",
				"constant.language.false"
			],
			"settings": {
				"foreground": "#A7F3D0"
			}
		},
		{
			"scope": [
				"constant.language",
				"constant.language.null",
				"constant.language.undefined",
				"constant.language.nan"
			],
			"settings": {
				"foreground": "#9CA3AF"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call.generic",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#67E8F9"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"variable.parameter",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#DBEAFE"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"support.class",
				"support.type",
				"storage.type.cs",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#5EEAD4"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.jsx",
				"entity.other.attribute-name.tsx"
			],
			"settings": {
				"foreground": "#DDD6FE"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.tag.html",
				"entity.name.tag.jsx",
				"entity.name.tag.tsx"
			],
			"settings": {
				"foreground": "#6EE7B7"
			}
		},
		{
			"scope": [
				"punctuation.definition.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#4A5568"
			}
		},
		{
			"scope": [
				"variable.language.this",
				"variable.language.self",
				"variable.language.super"
			],
			"settings": {
				"foreground": "#93C5FD",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta.object-literal.key",
				"meta.object.member",
				"variable.object.property"
			],
			"settings": {
				"foreground": "#99F6E4"
			}
		},
		{
			"scope": [
				"support.constant",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#FDE68A"
			}
		},
		{
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#F87171"
			}
		},
		{
			"scope": [
				"markup.heading",
				"entity.name.section.markdown"
			],
			"settings": {
				"foreground": "#99F6E4",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#6EE7B7",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#93C5FD",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.inline.raw",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "#67E8F9"
			}
		},
		{
			"scope": [
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#A7F3D0"
			}
		},
		{
			"scope": [
				"markup.quote.markdown"
			],
			"settings": {
				"foreground": "#6B7280",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"string.regexp",
				"constant.regexp"
			],
			"settings": {
				"foreground": "#FB923C"
			}
		},
		{
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#A5F3FC"
			}
		},
		{
			"scope": [
				"meta.import",
				"meta.export"
			],
			"settings": {
				"foreground": "#A5F3FC"
			}
		},
		{
			"scope": [
				"entity.name.function.decorator",
				"meta.decorator",
				"punctuation.decorator"
			],
			"settings": {
				"foreground": "#FDE68A"
			}
		},
		{
			"scope": [
				"support.type.property-name.json",
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#99F6E4"
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#C9D1D9"
			}
		},
		{
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#6B7280"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#DBEAFE",
		"parameter": "#DBEAFE",
		"function": "#67E8F9",
		"method": "#67E8F9",
		"class": "#5EEAD4",
		"interface": "#5EEAD4",
		"type": "#5EEAD4",
		"namespace": "#6EE7B7",
		"keyword": "#A5F3FC",
		"string": "#CCFBF1",
		"number": "#FEF08A",
		"boolean": "#A7F3D0",
		"comment": "#6B7280",
		"property": "#99F6E4",
		"enum": "#5EEAD4",
		"enumMember": "#FDE68A",
		"decorator": "#FDE68A",
		"macro": "#A5F3FC",
		"regexp": "#FB923C"
	}
}
